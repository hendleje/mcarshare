<!DOCTYPE HTML>
<html>
	<head>
		<title>M-CarShare</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="stylesheets/main.css" />
		<!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->
	</head>
	<body class="subpage">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header-wrapper">
					<header id="header" class="container">
						<div class="row">
							<div class="12u">

								<!-- Logo -->
									<h1><a href="findcar" id="logo">M-CarShare</a></h1>

								<<!-- Nav -->
									<nav id="nav">
										<h2><font color="white">Welcome {{firstName}} {{lastName}}!</font></h2>
										<h3><font color="white"><a href="logout">Logout</a></font></h3>
									</nav>

							</div>
						</div>
					</header>
				</div>
			
			<!-- Navigation -->
			<ul>
				<li><a href="findcar">Find Car</a></li>
				<li><a href="tripinformation">My Current Trip</a></li>
			</ul>

			<!-- Content -->
				<div id="content-wrapper">
					<div id="content">
						<div class="container">
							<div class="row">
								<div class="12u">

									<!-- Main Content -->
										<section>
											<header>
												<h2><font color="white">Directions to your reserved Car</font></h2>
											</header>
											<style>
											table#cars {
											    width:50%;
											    background-color: white;
											    text-align: right;
											}
											table#cars tr:nth-child(even) {
											    background-color: #f9f9f9;
											}
											
											table#cars th {
											    background-color: black;
											    color: white;
											}
											</style>
											<table id="cars">											  
												   <tr>
												    <td><iframe id="mapframe"
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src=https://www.google.com/maps/embed/v1/directions?key=AIzaSyCi9zdagO0xdTQVueKZDLbV5IJTPEU4IbA&origin=47.574454,-52.735613&destination=47.5635916,-52.712543" allowfullscreen>
</iframe></td></td>
												  </tr>
												</table>
												<form action="tripinformation" method = "post">
													<input type="submit" value="Check out car"/>
												</form>
										</section>

								</div>
							</div>
						</div>
					</div>
				</div>

			<!-- Copyright -->
				<div id="copyright">
					&copy; M-CarShare. All rights reserved. | M-CarShare
				</div>

		</div>

		<!-- Scripts -->
			<script src="/js/jquery.min.js"></script>
			<script src="/js/skel.min.js"></script>
			<script src="/js/skel-viewport.min.js"></script>
			<script src="/js/util.js"></script>
			<!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
			<script src="/js/main.js"></script>

	</body>
</html>